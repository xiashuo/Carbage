<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll1_set"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:visibility="visible"
    >
    <RelativeLayout
        android:id="@+id/rl1"
        style="@style/layout_style"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/bt_selector"
        >
        <ImageView
            android:id="@+id/ig1"
            style="@style/ig_style"
            android:src="@drawable/yjsz"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_alignParentLeft="true"
            />
        <TextView
            style="@style/tv_style"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/ig1"
            android:layout_marginLeft="10dp"
            android:text="一键收展"
            />
        <ImageView
            android:id="@+id/ig2"
            style="@style/ig_style"
            android:src="@drawable/jt"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            />
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/radus_shape"
        android:orientation="vertical"
        android:padding="1dp"
        >
        <RelativeLayout
            android:id="@+id/rl2"
            style="@style/layout_style"
            >
            <ImageView
                android:id="@+id/ig3"
                style="@style/ig_style"
                android:background="@drawable/jysd"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:layout_alignParentLeft="true"
                />
            <TextView
                style="@style/tv_style"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/ig3"
                android:layout_marginLeft="5dp"
                android:text="卷扬随动"
                />

            <Switch
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/switch2"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:thumb="@drawable/switch_selector"
                android:track="@drawable/track_selector"
                />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/myGrey"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            />
        <RelativeLayout
            android:id="@+id/rl3"
            style="@style/layout_style"
            android:background="@color/white"
            >
            <ImageView
                android:id="@+id/ig4"
                style="@style/ig_style"
                android:background="@drawable/bfbc"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:layout_alignParentLeft="true"
                />
            <TextView
                style="@style/tv_style"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/ig4"
                android:layout_marginLeft="5dp"
                android:text="变幅补偿"
                />

            <Switch
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/switch3"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:thumb="@drawable/switch_selector"
                android:track="@drawable/track_selector"
                />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/myGrey"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"

            />
        <RelativeLayout
            android:id="@+id/rl4"
            style="@style/layout_style"
            android:background="@color/white"
            >
            <ImageView
                android:id="@+id/ig5"
                style="@style/ig_style"
                android:background="@drawable/dgfb"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:layout_alignParentLeft="true"
                />
            <TextView
                style="@style/tv_style"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/ig5"
                android:layout_marginLeft="5dp"
                android:text="吊钩防摆"
                />

            <Switch
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/switch4"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:thumb="@drawable/switch_selector"
                android:track="@drawable/track_selector"
                />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/myGrey"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            />
        <RelativeLayout
            android:id="@+id/rl5"
            style="@style/layout_style"
            android:background="@color/white"
            >
            <ImageView
                android:id="@+id/ig6"
                style="@style/ig_style"
                android:background="@drawable/fdczy"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:layout_alignParentLeft="true"
                />
            <TextView
                style="@style/tv_style"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/ig6"
                android:layout_marginLeft="5dp"
                android:text="非对称作业"
                />

            <Switch
                android:layout_width="60dp"
                android:layout_height="20dp"
                android:id="@+id/switch5"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:thumb="@drawable/switch_selector"
                android:track="@drawable/track_selector"
                />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
